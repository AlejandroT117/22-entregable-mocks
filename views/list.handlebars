<table class="table">
  <thead>
    <tr>
      <th scope="col">Nombre</th>
      <th scope="col">Precio</th>
      <th scope="col">Imagen</th>
    </tr>
  </thead>
  <tbody id="tb">
    {{#each lista}}
      <tr>
        <td>{{this.nombre}}</td>
        <td>{{this.precio}}</td>
        <td><img src="{{this.img}}" alt="{{this.nombre}}"></td>
      </tr>
    {{/each}}
  </tbody>

</table>